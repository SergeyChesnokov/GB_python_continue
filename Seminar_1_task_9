# /////////////   Задача №9   ///////////////

print('\n\t\t\t\tТАБЛИЦА УМНОЖЕНИЯ\n\n')
for i in range(2, 11):
    for j in range(2, 6):
        print(f'{j} x {i}', ' '*(1 - i//10), f'= {i*j}', end = '\t\t')
    print('')

print('\n')
for i in range(2, 11):
    for j in range(6, 10):
        print(f'{j} x {i}', ' '*(1 - i//10), f'= {i*j}', end = '\t\t')
    print('')
print('\n'*2)

