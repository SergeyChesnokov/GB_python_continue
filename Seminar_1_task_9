# /////////////   Задача №9   ///////////////

print('\n\t\t\tТАБЛИЦА УМНОЖЕНИЯ\n')
for i in range(2, 10):
    for j in range(2, 6):
        print(f'{j} x {i} = {i*j}', end = '\t')
    print('')

print('\n')
for i in range(2, 10):
    for j in range(6, 10):
        print(f'{j} x {i} = {i*j}', end = '\t')
    print('')
print('\n'*2)

